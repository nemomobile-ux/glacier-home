<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="org.lipstick.polkitAuthAgent">
    <method name="initiateAuthenticationFinish">
      <arg type="b" direction="out"/>
    </method>
    <method name="cancelAuthentication">
    </method>
    <method name="sessionRequest">
      <arg name="request" type="s" direction="in"/>
      <arg name="echo" type="b" direction="in"/>
    </method>
    <method name="sessionComplete">
      <arg name="ok" type="b" direction="in"/>
    </method>
    <method name="finish">
    </method>
    <method name="initSession">
    </method>
    <method name="accepted">
    </method>
    <method name="rejected">
    </method>
  </interface>
</node>
